        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// a bunch of stuff

if (!global.paused or obj_hit_stun.stuned) {
    // user input
    UP    = keyboard_check(ord("W"))
    LEFT  = keyboard_check(ord("A"))
    DOWN  = keyboard_check(ord("S"))
    RIGHT = keyboard_check(ord("D"))
    
    DASH = keyboard_check_pressed(vk_space)
    
    SHOOTING = mouse_check_button(mb_left)
    
    direction_towards_mouse = point_direction(x, y, mouse_x, mouse_y)
    
    // cheats
    if (keyboard_check_pressed(ord("H"))) hit_points += 1
    
    // acceleration
    ax = 2 * (RIGHT - LEFT)
    ay = 2 * (DOWN - UP)
    
    // dashing
    can_dash++
    if (can_dash = dash_max_count) text_fx(x, y, "DASH READY")
    if (can_dash &gt; dash_max_count) can_dash = dash_max_count
    if (DASH and can_dash = dash_max_count) {
        dashing = true
        can_dash = 0
        dash_direction = direction_towards_mouse
        shake_camera(dash_duration, 10)
        alarm[2] = dash_duration
        audio_play_sound(sfx_dash, 0, 0)
    }
    
    // friction 
    // "What? In space?"
    // "Yep."
    vx -= sign(vx) * friction_strength
    vy -= sign(vy) * friction_strength
    
    // velocity
    if (dashing) {
        vx = lengthdir_x(dash_speed, dash_direction)
        vy = lengthdir_y(dash_speed, dash_direction)
    }
    else {
        vx += ax
        vy += ay
    }
    
    // don't let the player move faster than max_speed
    // unless if the player is dashing
    if (!dashing) {
        vx = clamp(vx, -max_speed, max_speed)
        vy = clamp(vy, -max_speed, max_speed)
    }
    
    // apply velocity to position
    x += vx
    y += vy
    
    // check if the player is not near the ufo
    if (instance_number(obj_ufo)) {
        wander = !within_view(obj_ufo.x, obj_ufo.y, obj_ufo.sprite_width)
    }
    // player is always within the room
    x = clamp(x, 0, room_width)
    y = clamp(y, 0, room_height)
    
    // player faces the cursor
    image_angle = direction_towards_mouse - 90
    
    // create a trail if the player is dashing
    if (dashing) ghost_trail(1)
    
    // reload
    if (gun_heat = max_gun_heat and !reloading) {
        reloading = true
        text_fx(x, y, "RELOADING")
    }
    else if (gun_heat = 0 and reloading) reloading = false
    
    // shooting
    can_shoot++
    if (!reloading) {
        if (!SHOOTING or dashing) gun_cooldown_rate(2)
        else if (can_shoot &gt; 2 - gun_upfirerate) {
            can_shoot = 0
            shake_camera(1, 2)
            gun_heat++
            for (i = -1; i &lt; 2; i++) {
                angle_diff = 15
                scatter = irandom_range(-12, 12)
                l = instance_create(x, y, obj_lazer_player)
                l.direction = image_angle + 90 + scatter + (i * angle_diff)
                l.image_angle = l.direction - 90
            }
            audio_play_sound(sfx_shoot_4, 0, 0)
        }
    }
    else gun_cooldown_rate(1)

} //end if (!global.paused)
</string>
